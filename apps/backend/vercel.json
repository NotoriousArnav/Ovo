{
  "version": 2,
  "framework": null,
  "installCommand": "cd ../.. && pnpm install",
  "buildCommand": "npx prisma generate",
  "outputDirectory": ".",
  "functions": {
    "api/index.ts": {
      "memory": 256,
      "maxDuration": 10,
      "includeFiles": "prisma/**,../../packages/shared/**"
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api" }
  ]
}
